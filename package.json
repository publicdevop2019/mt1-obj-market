{
  "name": "mt1-obj-market",
  "version": "0.0.0",
  "scripts": {
    "pretty-lint": "npm run prettier-ts && npm run lint && npm run prettier-html",
    "deploy": "ng build --prod && cp -a ./dist/mt1-obj-market/. ./public/ && rm -r ./public/* && cp -a ./dist/mt1-obj-market/. ./public/ && firebase deploy",
    "ng": "ng",
    "start": "ng serve",
    "start-zh-Hans": "ng serve --configuration=zh-Hans",
    "start-pwa": "http-server -p 8080 -c-1 dist/mt1-obj-market",
    "build": "ng build --configuration=production-en-US",
    "build-zh-Hans": "ng build --configuration=production-zh-Hans",
    "build-hw": "ng build --configuration=hw-en-US",
    "build-hw-zh-Hans": "ng build --configuration=hw-zh-Hans",
    "test": "ng test",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "i18n": "ng xi18n --output-path src/locale",
    "prettier-ts": "prettier --write \"src/{app,environment,assets}/**/*.ts\"",
    "prettier-html": "prettier --write \"src/app/**/*.component.html\" --parser angular"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.6",
    "@angular/cdk": "^9.1.2",
    "@angular/common": "~9.0.6",
    "@angular/compiler": "~9.0.6",
    "@angular/core": "~9.0.6",
    "@angular/forms": "~9.0.6",
    "@angular/localize": "^9.1.3",
    "@angular/material": "^9.1.2",
    "@angular/platform-browser": "~9.0.6",
    "@angular/platform-browser-dynamic": "~9.0.6",
    "@angular/platform-server": "~9.0.6",
    "@angular/router": "~9.0.6",
    "@angular/service-worker": "~9.0.6",
    "angular-in-memory-web-api": "^0.9.0",
    "hammerjs": "^2.0.8",
    "http-server": "^0.11.1",
    "qrcode": "^1.4.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "uuid": "^3.4.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.6",
    "@angular/cli": "~9.0.6",
    "@angular/compiler-cli": "~9.0.6",
    "@angular/language-service": "~9.0.6",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/qrcode": "^1.3.4",
    "@types/uuid": "^3.4.7",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.7.1",
    "typescript": "~3.7.5"
  }
}